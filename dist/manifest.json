{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "0.11.4",
  "default_locale": "en",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content_script.tsx-loader-CAxjV8Pe.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "assets/App.css"
      ]
    },
    {
      "js": [
        "assets/command_hub.tsx-loader-DXGomFeA.js"
      ],
      "matches": [
        "https://ujiro99.github.io/selection-command/*",
        "http://localhost:3000/*"
      ],
      "css": [
        "assets/command_hub.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "options_page": "src/options_page.html",
  "sandbox": {
    "pages": [
      "src/sandbox.html"
    ]
  },
  "action": {},
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/*.css",
        "assets/createLucideIcon-Bi_qrFlr.js",
        "assets/utils-4mZQsGzx.js",
        "assets/App-BbIsdbdr.js",
        "assets/icons-C0JEO2_s.js",
        "assets/analytics-CzMCqmRa.js",
        "assets/ipc-BR4eBWXN.js",
        "assets/content_script.tsx-CY7FIsv1.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://localhost:3000/*",
        "https://ujiro99.github.io/*"
      ],
      "resources": [
        "assets/createLucideIcon-Bi_qrFlr.js",
        "assets/utils-4mZQsGzx.js",
        "assets/ipc-BR4eBWXN.js",
        "assets/analytics-CzMCqmRa.js",
        "assets/command_hub.tsx-zVCN8ErN.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
